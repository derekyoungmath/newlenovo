# mod z
set $mode_system System (l) lock, (e) logout, (s) suspend, (h) hibernate, (r) reboot, (Shift+s) shutdown
mode "$mode_system" {
    bindsym z exec --no-startup-id xset s on & xset dpms force standby, mode "default"
    bindsym l exec --no-startup-id i3lock-fancy, mode "default"
    bindsym e exec --no-startup-id i3-msg exit logout, mode "default"
    bindsym s exec --no-startup-id i3lock-fancy -t "Compute Hard!!" && systemctl suspend, mode "default"
    bindsym h exec --no-startup-id i3lock-fancy && systemctl hibernate, mode "default"
    bindsym r exec --no-startup-id systemctl reboot, mode "default"
    bindsym Shift+s exec --no-startup-id systemctl poweroff, mode "default"
    # back to normal: Enter or Escape
    bindsym Return mode "default"
    bindsym Escape mode "default"
}
bindsym $mod+z mode "$mode_system"

# mod x
set $mode_net Net (j) up Duval, (k) up ISU, (l) down Duval, (h) down ISU, (i) all, (s) ssh home
mode "$mode_net" {
    bindsym j exec --no-startup-id nmcli c up "Duval 1", mode "default"
    bindsym k exec --no-startup-id nmcli c up IASTATE, mode "default"
    bindsym l exec --no-startup-id nmcli c down Duval 1, mode "default"
    bindsym h exec --no-startup-id nmcli c down IASTATE, mode "default"
    bindsym i exec --no-startup-id terminator -e nmcli d wifi list && echo "press enter to exit" && read, mode "default"
    # back to normal: Enter or Escape
    bindsym Return mode "default"
    bindsym Escape mode "default"
}
bindsym $mod+x mode "$mode_net"

