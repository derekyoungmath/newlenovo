# vim: filetype=config
defaults
protocol smtp
auth on
tls on
#tls_nocertcheck
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/.msmtp.log

#Below are example accounts. Comment out what you don't use.

#Here is an example gmail account.
account gmail
from ddyoung@iastate.edu
user ddyoung@iastate.edu
passwordeval "gpg2 -q --for-your-eyes-only --no-tty -d ~/.password-store/passnew.gpg"
host smtp.gmail.com
port 587

